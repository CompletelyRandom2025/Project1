{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["ESNext", "DOM"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "exclude": ["dist"]
}
